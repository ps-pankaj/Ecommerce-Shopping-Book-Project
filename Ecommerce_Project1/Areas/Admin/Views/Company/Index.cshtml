<div class="row">
<div class="col-9">
    <h2 class="text-primary">Company List</h2>
</div>
<div class="col-3">
    <a asp-action="Upsert" class="btn btn-info form-control">
        <i class="fas fa-plus"></i> &nbsp; Add New Company
    </a>
</div>
</div>
<div class="container rounded border m-2 p-2">
    <table id="tblData" class="table table-striped table-bordered table-active">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Phone Number</th>
                <th>Is Authorized</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts
{
    <script src="~/js/Company.js"></script>
}